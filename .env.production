# Application
NODE_ENV=production
PORT=3000
NEXT_PUBLIC_APP_URL=https://heart-lab-ai.netlify.app

# Security
ALLOWED_ORIGINS=https://heart-lab-ai.netlify.app
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info

# Redis
REDIS_URL=${REDIS_URL}

# Monitoring
METRICS_ENABLED=true
METRICS_INTERVAL=60000

# Health Check
HEALTH_CHECK_INTERVAL=30000
HEALTH_CHECK_TIMEOUT=5000